package 解题方法;

public class Solution {
	public boolean isAlienSorted(String[] words, String order) {
		boolean res = true;
		for (String word : words) {
			for (int i = 0; i < word.toCharArray().length; i++) {
				int a = order.indexOf(word.toCharArray()[i]);
				for (int j = i+1; j < word.toCharArray().length -1 ; j++) {
					int b = order.indexOf(word.toCharArray()[j]);
					if(a > b){
						res = false;
						break;
					}
				}
				if(!res){
					break;
				}
			}
			if(!res){
				break;
			}
		}
		return res;
	}
}
